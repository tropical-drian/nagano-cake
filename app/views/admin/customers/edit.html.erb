<h2><%= full_name(@customer) %>さんの会員情報編集</h2>
<%= form_with model: [:admin, @customer], local: true  do |f| %>
<div class="form-group col-xs-9">
  <%= f.label :"会員ID" %>
  <%= f.label :"#{@customer.id}" %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"氏名" %>
  <%= f.text_field :last_name %>　
  <%= f.text_field :first_name %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"フリガナ" %>
  <%= f.text_field :kana_last_name %>
  <%= f.text_field :kana_first_name %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"郵便番号", autofocus: true %>
  <%= f.text_field :postal_code %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"住所" %>
  <%= f.text_field :residence, autofocus: true %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"電話番号" %>
  <%= f.text_field :phone_number %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"メールアドレス" %>
  <%= f.text_field :email %>
</div>
<div class="form-group col-xs-9">
  <%= f.label :"会員ID" %>
  <%= f.label :"#{@customer.id}" %>
</div>
<div>会員ステータス</div>
<div class="actions">
  <%= f.submit '変更を保存' ,class: "btn btn-primary" %>
</div>
<% end %>